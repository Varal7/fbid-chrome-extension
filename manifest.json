{
    "manifest_version": 2,

    "name": "Fb ID",
    "description": "Display Facebook Id of current page",
    "version": "1.1",

  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["jquery.js", "content.js"]
    }
  ],

    "browser_action": {
        "default_icon": "book.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}
